<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bkredfox.minhhien.matrixcal_demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".matrixDisplay"
            android:label="@string/MatrixDisplay"/>
        <activity android:name=".menu"
            android:label="@string/Menu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".setting"
            android:label="@string/setting"/>
        <activity android:name=".storage"
            android:label="@string/storage"/>
        <activity android:name=".functions"
            android:label="@string/Function"></activity>
    </application>

</manifest>